{
  "name": "create-t3-turbo",
  "private": true,
  "engines": {
    "node": ">=20.15.0"
  },
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "db:push": "turbo -F @dict/db push",
    "db:studio": "turbo -F @dict/db studio",
    "dev": "turbo watch dev --continue",
    "dev:next": "turbo watch dev -F @dict/nextjs...",
    "format": "turbo run format --continue -- --cache --cache-location .cache/.prettiercache",
    "format:fix": "turbo run format --continue -- --write --cache --cache-location .cache/.prettiercache",
    "lint": "turbo run lint --continue -- --cache --cache-location .cache/.eslintcache",
    "lint:fix": "turbo run lint --continue -- --fix --cache --cache-location .cache/.eslintcache",
    "lint:ws": "pnpm dlx sherif@latest",
    "postinstall": "pnpm lint:ws",
    "typecheck": "turbo run typecheck",
    "ui-add": "turbo run ui-add",
    "supabase:start": "turbo -F @dict/supabase start",
    "supabase:stop": "turbo -F @dict/supabase stop",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "preview": "pnpm build && turbo run start",
    "preview:next": "pnpm build && turbo run start -F @dict/nextjs..."
  },
  "devDependencies": {
    "@dict/prettier-config": "workspace:*",
    "@turbo/gen": "^2.1.1",
    "prettier": "catalog:",
    "turbo": "^2.1.1",
    "typescript": "catalog:"
  },
  "prettier": "@dict/prettier-config"
}